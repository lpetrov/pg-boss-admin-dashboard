# PostgreSQL connection string (required)
# Format: postgres://username:password@host:port/database
PGBOSS_DATABASE_URL=postgres://user:password@localhost:5432/your_database

# Dashboard port (optional, default: 8671)
PGBOSS_DASHBOARD_PORT=8671

# Disable auto-opening browser (optional, default: false)
# PGBOSS_NO_BROWSER=true